{
  "name": "ipld-dag-pb",
  "version": "0.11.0",
  "description": "JavaScript Implementation of the MerkleDAG Node in Protobuf.",
  "main": "src/index.js",
  "scripts": {
    "lint": "aegir-lint",
    "build": "npm run build-proto && aegir-build",
    "build-proto": "pbjs --wrap commonjs --target static-module src/dag.proto > src/dag.proto.js",
    "test": "aegir-test",
    "test:node": "aegir-test node",
    "test:browser": "aegir-test browser",
    "release": "npm run build-proto && aegir-release",
    "release-minor": "npm run build-proto && aegir-release --type minor",
    "release-major": "npm run build-proto && aegir-release --type major",
    "coverage": "aegir-coverage"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "author": "Vijayee Kulkaa <vijayee.kulkaa@hushmail.com>",
  "contributors": [
    "David Dias <daviddias.p@gmail.com>",
    "Friedel Ziegelmayer <dignifiedquire@gmail.com>",
    "James Halliday <mail@substack.net>",
    "Jeff Downie <JeffDownie@users.noreply.github.com>",
    "Mitar <mitar.github@tnode.com>",
    "Richard Littauer <richard.littauer@gmail.com>",
    "Stephen Whitmore <stephen.whitmore@gmail.com>",
    "Vijayee Kulkaa <vijayee.kulkaa@hushmail.com>",
    "dmitriy ryajov <dryajov@dmitriys-MacBook-Pro.local>",
    "dryajov <dryajov@gmail.com>",
    "greenkeeper[bot] <greenkeeper[bot]@users.noreply.github.com>",
    "haad <haad@headbanggames.com>",
    "nginnever <ginneversource@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipld/js-ipld-dag-pb.git"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "async": "^2.1.5",
    "bs58": "^4.0.0",
    "buffer-loader": "0.0.1",
    "cids": "~0.5.0",
    "ipfs-block": "~0.6.0",
    "is-ipfs": "~0.3.0",
    "multihashes": "~0.4.4",
    "multihashing-async": "~0.4.4",
    "protobufjs": "^6.8.0",
    "pull-stream": "^3.5.0",
    "pull-traverse": "^1.0.3",
    "stable": "^0.1.6"
  },
  "devDependencies": {
    "aegir": "^11.0.0",
    "chai": "^3.5.0",
    "chai-checkmark": "^1.0.1",
    "detect-node": "^2.0.3",
    "dirty-chai": "^1.2.2",
    "ipfs-block-service": "~0.9.0",
    "ipfs-repo": "~0.12.0",
    "lodash": "^4.17.4",
    "ncp": "^2.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1"
  }
}
