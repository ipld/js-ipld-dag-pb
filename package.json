  "description": "JavaScript Implementation of the MerkleDAG Node in Protobuf.",
  "leadMaintainer": "Volker Mische <volker.mische@gmail.com>",
  "main": "src/index.js",
  "scripts": {
    "prepare": "pbjs ./src/dag.proto -t json -o ./src/dag.json && aegir build --no-bundle",
